  "scripts": {
    "version:check": "node -r ./launcher/utils/error-handler ./launcher/commands/version-check",
    "data:move": "node -r ./launcher/utils/error-handler ./launcher/commands/data-move",
    "dll:dev": "cross-env NODE_ENV=development node -r ./launcher/utils/error-handler ./launcher/commands/dll-dev",
    "dll:build": "cross-env NODE_ENV=production node -r ./launcher/utils/error-handler ./launcher/commands/dll-prod",
    "build:clean": "node -r ./launcher/utils/error-handler ./launcher/commands/build-clean",
    "build:app": "node -r ./launcher/utils/error-handler ./launcher/commands/build-app",
    "server:dev": "cross-env NODE_ENV=development node -r ./launcher/utils/error-handler ./launcher/server/app-dev",
    "server:build": "cross-env NODE_ENV=production node -r ./launcher/utils/error-handler ./launcher/server/app-prod",
    "dev": "npm run dll:dev && npm run server:dev",
    "build": "npm run build:clean && npm run dll:build && run-p data:move build:app",
    "analyse": "npm run build:clean && node -r ./launcher/utils/error-handler ./launcher/commands/analyse"
  },
